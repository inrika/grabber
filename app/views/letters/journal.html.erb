<h1> Журнал</h1>
<table class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
      <th>account</th>
      <th>Дата копирования</th>
      <th>Количество писем</th>

    </tr>
   </thead>

   <tbody >
    <% @journal.each do|k,v|%>
      <%v.each do |k1,v1|%>
        <tr>
          <td><%=k.email%> </td>
          <td><%= k1%></td>
          <td><%= v1%></td>

      </tr>
      <%end%>
    <%end%>
  </tbody>
</table>
